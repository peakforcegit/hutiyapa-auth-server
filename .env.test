NODE_ENV=test
PORT=3001

# Database (In-memory SQLite for testing - but schema is PostgreSQL)
# We'll mock the database calls for E2E tests
DATABASE_URL="postgresql://test:test@localhost:5432/auth_test?schema=public"

# Security
JWT_ACCESS_SECRET=test-jwt-access-secret-key-for-testing-minimum-32-characters-long
JWT_REFRESH_SECRET=test-jwt-refresh-secret-key-for-testing-minimum-32-characters-long
COOKIE_SECRET=test-cookie-secret-for-testing-minimum-32-characters-long

# Frontend/CORS
FRONTEND_URL=http://localhost:3001
WEB_APP_URL=http://localhost:3001
